//NANETTE AVIGNONE CST112
//CREATE AN ARRAY AND FIND LARGEST NUMBER AND MOVE TO BOTTOM

int students[] ; 

void setup() {
  size(300, 300);
  students = new int[10];
  
  for (int i = 0; i <= 9; i++) {
    students[i] = int (random (0, 50));
    
    
  }
  text (students[9], 20, 20);
 // background(255);
  //noLoop();
}
